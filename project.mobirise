          "_name": "features12",
          "_customHTML": "<section class=\"features12\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" value=\"4\" step=\"1\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" value=\"4\" step=\"1\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked=\"\">\n        \n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#bbc3cc\" condition=\"showLine\">\n        <input type=\"range\" inline=\"\" title=\"Items Count\" name=\"itemsCount\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\">\n        <input type=\"range\" inline=\"\" title=\"Icons Count\" name=\"iconsCount\" min=\"1\" max=\"2\" step=\"1\" value=\"1\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#bbc3cc\" condition=\"showIcons\">\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Item Title\" name=\"showItemTitle\">\n        <input type=\"checkbox\" title=\"Show Item Text\" name=\"showItemText\">\n        <input type=\"color\" title=\"Cards BG Color\" name=\"cardsBgColor\" value=\"#ffffff\">\n        <input type=\"checkbox\" title=\"Show Button\" name=\"showButton\">\n\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#f6efe9\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row top-row\">\n            <div class=\"text-block mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-12\">\n                <div class=\"text-wrap\">\n                    <h3 class=\"mbr-section-title mbr-fonts-style mbr-white mbr-bold align-center mbr-mb-8\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .line \">\n                       INIT Cambridge</h3>\n                    <div class=\"line align-center\">\n                        <div mbr-if=\"showLine \" class=\"line-bottom\"></div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"mbr-row bottom-row\">\n            <div mbr-list=\"\" mbr-list-grow=\"itemsCount\" class=\"list-wrap width-full mbr-flex-wrap mbr-justify-content-center d-flex mbr-mt-50\">\n                <div class=\"list-item mbr-flex-column d-flex mbr-justify-content-center mbr-align-items-center mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12 mbr-col-lg-6':itemsCount==1||itemsCount==2,\n                     'mbr-col-md-12 mbr-col-lg-4':itemsCount==3}\">\n                    <div class=\"content width-full\">\n                        <div class=\"card-img\" mbr-if=\"showImg\">\n                            <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/cie-logo-338x113.png\" layout=\"responsive\" width=\"370.9026548672567\" height=\"124\" alt=\"Mobirise\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/cie-logo-338x113.png\" alt=\"Mobirise\" title=\"\">\n                            </amp-img>\n                        </div>\n                        <div class=\"content-wrapper mbr-p-16\">\n                            <div class=\"content-top d-flex\" mbr-class=\"{'mbr-justify-content-between':showItemTitle==true,\n                             'mbr-justify-content-end': showItemTitle==false}\">\n                                <h4 class=\"item-title align-left mbr-fonts-style mbr-black mbr-bold mbr-mt-8\" data-app-selector=\".item-title\" mbr-if=\"showItemTitle\" mbr-theme-style=\"display-6\">\n                                    English Course\n                                </h4>\n                                <div class=\"icons-wrapper d-flex\" mbr-if=\"showIcons\" mbr-list=\"\" mbr-list-grow=\"iconsCount\">\n                                    <div class=\"icon-item d-flex\">\n                                        <div class=\"icon-wrap\" mbr-if=\"showIcons\">\n                                            <span mbr-icon=\"\" class=\"mbr-iconfont fa fa-user\"></span>\n                                         </div>\n                                         <p class=\"room mbr-fonts-style mbr-bold\" data-app-selector=\".room\" mbr-theme-style=\"display-7\" mbr-if=\"showIcons\">23</p>\n                                    </div>\n                                    \n                                </div>\n                            </div>\n                            <div class=\"content-bottom\">\n                                <p class=\"item-text grey-color align-left mbr-text mbr-semibold mbr-fonts-style mbr-mt-8\" mbr-theme-style=\"display-7\" mbr-if=\"showItemText\" data-app-selector=\".item-text\">\n                                    21.08.2019\n                                </p>\n                     